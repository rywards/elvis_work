#include <stdio.h>


int main(){

  int x = 10;
  int iterator;
  while ( x > 0){

    if (x % 3 == 0){
      printf("%d is divisible by 3!");
    }
    printf("This is gonna get messy! %d\n", iterator++);
    
  }
  
}
