(sumlist '(3 7 4))
(+ 3 (sumlist '(7 4)))
(+ 3 (+ 7 (sumlist '(4))))
(+ 3 (+ 7 (+ 4 (sumlist '( )))))
---------------------------------
(+ 3 (+ 7 (+ 4 0)))
(+ 3 (+ 7 4))
(+ 3 11)
14

(add_pairs '(1 3) '(2 10))
(cons 3 (add_pairs '(3) '(10))))
(cons 3 (cons 13 (add_pairs '( ) '( ))))
------------------------------------------
(cons 3 (cons 13 '( )))
(cons 3 '(13))
(3 13)

